{
  "$schema": "http://json-schema.org/schema",
  "cli": "nx",
  "description": "Serve the app for development",
  "outputCapture": "direct-nodejs",
  "properties": {
    "host": {
      "default": "localhost",
      "description": "Dev server hostname",
      "type": "string"
    },
    "migrate": {
      "default": true,
      "description": "Run database migration before serving",
      "type": "boolean"
    },
    "port": {
      "default": 3000,
      "description": "Dev server port",
      "type": "number"
    }
  },
  "required": [],
  "title": "Dev Server",
  "type": "object",
  "version": 2
}
