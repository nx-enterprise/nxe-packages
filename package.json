  "scripts": {
    "contributors.add": "all-contributors add",
    "contributors.generate": "all-contributors generate",
    "npm.publish": "nx run-many --target=build --all --parallel=3 && npx concurrently 'npm:npm.publish.*'",
    "npm.publish.nxe-dapr": "npm publish ./dist/packages/nxe-dapr/ --access=public",
    "npm.publish.nxe-ddd-architecture": "npm publish ./dist/packages/nxe-ddd-architecture/ --access=public",
    "npm.publish.nxe-php": "npm publish ./dist/packages/nxe-php/ --access=public",
    "npm.publish.nxe-python": "npm publish ./dist/packages/nxe-python/ --access=public",
    "npm.publish.nxe-python-fastapi": "npm publish ./dist/packages/nxe-python-fastapi/ --access=public",
    "prepare": "husky install",
    "release.major": "nx run workspace:version --skipProjectChangelog --syncVersions --tagPrefix '' --origin github --releaseAs=major",
    "release.minor": "nx run workspace:version --skipProjectChangelog --syncVersions --tagPrefix '' --origin github --releaseAs=minor",
    "release.patch": "nx run workspace:version --skipProjectChangelog --syncVersions --tagPrefix '' --origin github --releaseAs=patch",
    "sort-package-json": "npx sort-package-json"
  },
